model Qualification {
  id        String @id @default(uuid())
  tutorId   String
  tutor     User   @relation(fields: [tutorId], references: [id])
  exam      String @db.Text
  year      String @db.Text
  gpa       String @db.Text
  group     String @db.Text
  institute String @db.Text

  @@index([tutorId])
}
