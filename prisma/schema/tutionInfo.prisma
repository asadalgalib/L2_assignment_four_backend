model TutionInfo {
  id            String   @id @default(uuid())
  tutorId       String   @unique
  tutor         User     @relation(fields: [tutorId], references: [id])
  availability  Boolean  @default(true)
  salary        Int
  subjects      String[]
  availableTime String
  startDate     DateTime
  endDate       DateTime

  @@index([tutorId])
}
