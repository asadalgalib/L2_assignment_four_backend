model TutionInfo {
  id            String       @id @default(uuid())
  tutorId       String       @unique
  tutorProfile  TutorProfile @relation(fields: [tutorId], references: [tutorId])
  availability  Boolean      @default(true)
  salary        Int
  subjects      String[]
  availableTime String
  startDate     DateTime
  endDate       DateTime

  @@index([tutorId])
}
